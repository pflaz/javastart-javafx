<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.javastart.youtufy.controller.ControlPaneController" BorderPane.alignment="CENTER">
    <children>
        <Button fx:id="previousButton" mnemonicParsing="false" text="Button" />
        <ToggleButton fx:id="playButton" mnemonicParsing="false" text="Button" />
        <Button fx:id="nextButton" mnemonicParsing="false" text="Button" />
        <Slider fx:id="volumeSlider" />
        <Slider fx:id="songSlider" />
    </children></HBox>
